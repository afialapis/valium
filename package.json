{
  "name": "valium",
  "version": "0.0.31",
  "description": "A Form validator for React, using and customizing HTML5 Constraint Validation API",
  "author": "Donato Lorenzo <donato@afialapis.com>",
  "contributors": [
    "Donato Lorenzo <donato@afialapis.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afialapis/valium.git"
  },
  "bugs": {
    "url": "https://github.com/afialapis/valium/issues"
  },
  "homepage": "https://valium.afialapis.com/",
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "browser": "dist/valium.umd.js",
  "module": "dist/valium.es.js",
  "scripts": {
    "lint": "eslint src",
    "test": "karma start test/karma.conf.js",
    "test-run": "karma start test/karma.conf.js --no-single-run",
    "clean-lib": "rimraf dist && mkdir dist",
    "lib": "npm run clean-lib && babel src -d lib",
    "clean-dist": "rimraf dist && mkdir dist",
    "old_build-dist": "webpack",
    "old_build-dist-min": "NODE_ENV=production webpack",
    "old_dist": "npm run clean-dist && npm run build-dist && npm run build-dist-min",
    "dist": "rollup -c",
    "prepublish": "npm run test && npm run lib && npm run dist",
    "demo": "NODE_ENV=development node demo/server"
  },
  "keywords": [
    "js",
    "react",
    "form",
    "validation"
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "co": "^4.6.0",
    "css-loader": "^3.6.0",
    "es6-promise": "^4.2.8",
    "eslint": "^6.8.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.2.3",
    "node-sass": "^4.14.1",
    "node-uuid": "^1.4.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-uglify": "^6.0.4",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^2.3.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "prop-types": ">=15.7.2"
  }
}
